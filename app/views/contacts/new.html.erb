<h1>お問い合わせ</h1>

<div>
  <%= form_for(@contact, url: {action: 'confirm'}) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.label :content %>
    <%= f.text_area :content %>

    <%= f.hidden_field :circle_id, :value => params[:circle_id] %>

    <%= f.submit "確認" %>
  <% end %>
</div>