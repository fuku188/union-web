<div class="wrap-search ta_center">
  <%= link_to "フィード", [:feed, :circles] , class: "button button_feed button_feed_current"%>
  <%= link_to "検索", [:search, :circles] , class: "button button_search"%>
</div>

<% if @circles.any? %>
  <% @circles.each do |circle| %>
    <div class="wrap_card">
      <%= render "circle_card", circle: circle %>
      <div id="favorite_form_<%= circle.id %>">
        <% @circle = circle %>
        <%= render !current_user.favorites.find_by(circle_id: @circle.id) ? "favorite" : "unfavorite" , circle: @circle %>
      </div>
    </div>
  <% end %>
<% end %>